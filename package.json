{
	"name": "futurenhs",
	"version": "1.0.0",
	"description": "This repository contains artefacts pertinent to the development of the new FutureNHS sharing platform",
	"main": "gulpfile.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"prepare": "husky install"
	},
	"author": "",
	"license": "ISC",
	"repository": {
		"azureFeed": "cdsbinaryfeed",
		"azureOrganisation": "TBC",
		"defaults": {
			"organisation": "TBC",
			"project": "Future NHS"
		}
	},
	"binaries": [
		{
			"name": "nuget",
			"version": "4.8.1",
			"extension": "zip",
			"path": "./.nuget/nuget.zip",
			"detect": "./.nuget/nuget.exe",
			"install": [
				"unzip -o ./.nuget/nuget.zip -d ./.nuget"
			]
		}
	],
	"nuget": {
		"repository": "ignore",
		"packages": [
			{
				"name": "MVCForum.Website",
				"directory": "MVCForum.Website",
				"ignore": [
					"node_modules/**/*"
				]
			}
		]
	},
	"lint-staged": {
		"*": "prettier --write"
	},
	"dependencies": {
		"gulp": "4.0.2",
		"husky": "8.0.1",
		"pm2": "^5.2.0"
	}
}
